# Ignore Python cache and environment files
**/__pycache__/
*.pyc
*.pyo
*.env
.env*
venv/
.venv/

# Ignore credentials and environment variables
env_vars*
credentials*

# Ignore build artifacts
**/build/

# Project-specific ignores
# data/ # Replaced with specific rules for DVC
reports/
models/
notebooks/executed/
.DS_Store

# DVC/MLflow specific ignores
# Ignore the DVC cache and local remote storage
dvc_storage/

# Ignore MLflow experiment tracking data
mlruns/

# Ignore the contents of data directories, but not the directories themselves
# This allows Git to track the .dvc files created by DVC
data/anonymisedData/*
!data/anonymisedData/.gitkeep
data/processed/*
!data/processed/.gitkeep
data/interim/*
!data/interim/.gitkeep

# Ignore large data and model artifact files
*.csv
*.zip
*.db
*.pkl
*.h5
*.json
*.xlsx
*.sav
*.npz
*.npy
*.parquet
*.feather
*.joblib
*.pt
*.onnx
*.model
*.weights

# Allow source code and documentation
!src/
!scripts/
!docs/
!*.md
!*.txt
!pyproject.toml
!poetry.lock

# Allow this .gitignore
!.gitignore